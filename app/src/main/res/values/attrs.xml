<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IconTextView">
        <attr name="src" format="reference"/>
        <attr name="imagePadding" format="dimension"/>
        <attr name="text" format="string"/>
        <attr name="textColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="SliderSwitchView">
        <attr name="fontSize" format="dimension"/>
        <attr name="fontColor" format="color"/>
        <attr name="fontColorFocused" format="color"/>
        <attr name="leftFont" format="string"/>
        <attr name="rightFont" format="string"/>
        <attr name="lineColor" format="color"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="lineHeightMargin" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="VoiceIconView">
        <attr name="lineWidth" format="dimension"/>
        <attr name="color" format="color"/>
        <attr name="colorActive" format="color"/>
    </declare-styleable>

    <declare-styleable name="CustomContainerView">
        <attr name="bg" format="reference"/>
    </declare-styleable>

    <declare-styleable name="AddressView">
        <attr name="smallColor" format="color"/>
        <attr name="bigColor" format="color"/>
        <attr name="midGap" format="dimension"/>
        <attr name="bigSize" format="dimension"/>
        <attr name="smallSize" format="dimension"/>
        <attr name="bigText" format="string"/>
        <attr name="smallText" format="string"/>
    </declare-styleable>

    <declare-styleable name="LetterPickerView">
        <attr name="letterColor" format="color"/>
        <attr name="letterSize" format="dimension"/>
    </declare-styleable>
</resources>